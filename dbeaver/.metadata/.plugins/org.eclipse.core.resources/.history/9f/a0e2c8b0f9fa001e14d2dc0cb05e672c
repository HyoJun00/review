SELECT *
FROM PLAYER
WHERE WEIGHT BETWEEN 70 AND 80
ORDER BY WEIGHT DESC;

SELECT *
FROM PLAYER
WHERE TEAM_ID = 'K03' AND HEIGHT < 180
ORDER BY HEIGHT DESC;

SELECT *
FROM PLAYER
WHERE TEAM_ID = 'K06' AND NICKNAME = '제리'
ORDER BY HEIGHT DESC;

SELECT PLAYER_NAME 선수이름
FROM PLAYER
WHERE WEIGHT > 80 AND HEIGHT > 170
ORDER BY HEIGHT DESC;