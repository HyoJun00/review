CREATE TABLE TBL_PROFESSOR(
	PROFESSOR_NUMBER NUMBER,
	PROFESSOR_NAME VARCHAR2(100),
	PROFESSOR_MAJOR VARCHAR2(100),
	PROFESSOR_POSITION VARCHAR2(100),
	CONSTRAINT PK_PROFESSOR PRIMARY KEY(PROFESSOR_NUMBER)
);

SELECT * FROM TBL_PROFESSOR;

CREATE TABLE TBL_STUDENT(
	STUDENT_NUMBER NUMBER,
	STUDENT_NAME VARCHAR2(100),
	STUDENT_GRADE NUMBER,
	STUDENT_SCORE VARCHAR2(50),
	PROFESSOR_NUMBER NUMBER,
	CONSTRAINT PK_STUDENT PRIMARY KEY(STUDENT_NUMBER),
	CONSTRAINT FK_STUDENT FOREIGN KEY(PROFESSOR_NUMBER) REFERENCES TBL_PROFESSOR(PROFESSOR_NUMBER)
);

SELECT * FROM TBL_STUDENT;

CREATE TABLE TBL_SUBJECT(
	SUBJECT_NUMBER NUMBER,
	SUBJECT_NAME VARCHAR2(100),
	SUBJECT_CREDITVARCHAR2(100),
	STUDENT_NUMBER NUMBER,
	PROFESSOR_NUMBER NUMBER,
	CONSTRAINT PK_SUBJECT PRIMARY KEY(SUBJECT_NUMBER),
	CONSTRAINT FK_SUBJECT1 FOREIGN KEY(PROFESSOR_NUMBER) REFERENCES TBL_PROFESSOR(PROFESSOR_NUMBER),
	CONSTRAINT FK_STUDENT2 FOREIGN KEY(STUDENT_NUMBER) REFERENCES TBL_STUDENT(STUDENT_NUMBER)
)

